{
  "name": "find-my-domain",
  "version": "2.0.0",
  "description": "AI-powered domain name generator with real-time WHOIS availability checking",
  "private": false,
  "author": {
    "name": "Dmitrii Selikhov",
    "email": "selikhov.dmitrey@gmail.com",
    "url": "https://www.linkedin.com/in/dimetrix"
  },
  "repository": {
